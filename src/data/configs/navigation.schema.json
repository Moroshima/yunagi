{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "navigation": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "en": {
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": ["url", "name", "en"]
      }
    }
  },
  "additionalProperties": false,
  "required": ["$schema", "navigation"]
}
